<div id="loginPage">
    <div class="brand-box">
        <img class="logo" src="images/whos_the_most_wanted.jpg">
    </div>
    <form id="loginForm" role="form" class="sign-up-form">
        <div class="form-group">
            <input id="userName" name="userName" type="text" class="form-control" placeholder="Email">
        </div>
        <div class="form-group">
            <input id="password" name="password" type="password" class="form-control" placeholder="Password">
        </div>
        <button type="submit" class="btn btn-custom btn-block">OK</button>
    </form>
</div>
